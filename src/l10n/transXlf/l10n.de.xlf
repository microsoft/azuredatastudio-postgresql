        <target state="translated">Befehlszeilenoptionen</target>
      </trans-unit>
      <trans-unit id="postgres.connectionOptions.options.description">
        <source xml:lang="en">Command-line options to send to the server when the connection starts</source>
        <target state="translated">Befehlszeilenoptionen, die beim Starten der Verbindung an den Server gesendet werden sollen</target>
      </trans-unit>
      <trans-unit id="postgres.connectionOptions.applicationName.displayName">
