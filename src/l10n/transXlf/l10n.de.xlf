  <file original="out/dotnet" source-language="en" datatype="plaintext" target-language="de">
    <body>
      <trans-unit id="extension.installDotNetCoreButtonText">
        <source xml:lang="en">Install .NET Core SDK...</source>
        <target state="translated">.NET Core SDK installieren...</target>
      </trans-unit>
      <trans-unit id="extension.dotNetCoreNotFoundMessage">
        <source xml:lang="en">The .NET Core SDK was not found.</source>
        <target state="translated">Das .NET Core SDK wurde nicht gefunden.</target>
      </trans-unit>
